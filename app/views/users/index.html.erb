<div class="index">

  <div class ="search_container">
    <%= form_tag(users_path, method: :get) do %>
    <%= text_field_tag :search, params[:search], placeholder: "Search Chefs" %>
    <%= submit_tag 'Search', name: nil, :class => "search_button" %>
    <% end %>
  </div>

  <div class="list home">
    <% @users.each do |user| %>
    <div class="chef-list">
      <%= link_to "#{user.name}", user_path(user) %> <br>
      <button class="button show"><%= link_to "Show", user_path(user)%> </button>
    </div>
    <% end %>
  </div>


